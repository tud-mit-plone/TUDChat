<?xml version="1.0"?>
<object name="ChatSession"
        meta_type="Factory-based Type Information with dynamic views"
        i18n:domain="plone"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n">
    <property name="title">ChatSession</property>
    <property name="description"></property>
    <property name="content_icon"></property>
    <property name="content_meta_type">ChatSession</property>
    <property name="product">tud.addons.chat</property>
    <property name="factory">addChatSession</property>
    <property name="immediate_view">view</property>
    <!-- parent-child relation -->
    <property name="global_allow">False</property>
    <property name="filter_content_types">False</property>
    <!-- allow discussion -->
    <property name="allow_discussion">False</property>
    <!-- set default view-->
    <property name="default_view">view</property>
    <!-- available view in view-selection -->
    <property name="view_methods">
        <element value="view"/>
    </property>
    <alias from="(Default)"
           to="(dynamic view)"/>
    <alias from="edit"
           to="atct_edit"/>
    <alias from="sharing"
           to="@@sharing"/>
    <alias from="view"
           to="(selected layout)"/>
    <action title="View"
            action_id="view"
            category="object"
            condition_expr=""
            url_expr="string:${object_url}/"
            visible="True">
        <permission value="View"/>
    </action>
    <action title="Edit"
            action_id="edit"
            category="object"
            condition_expr=""
            url_expr="string:${object_url}/edit"
            visible="True">
        <permission value="Modify portal content"/>
    </action>
</object>