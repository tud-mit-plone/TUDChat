            cookable="True"
            enabled="True"
            expression="python: here.restrictedTraverse('@@plone_interface_info').provides('tud.addons.chat.interfaces.IChatSession')"
            inline="False"
            insert-after="*"/>

    <javascript id="++resource++tud.addons.chat/hyperlink.js"
            cacheable="True"
            compression="safe"
            cookable="True"
            enabled="True"
            expression="python: here.restrictedTraverse('@@plone_interface_info').provides('tud.addons.chat.interfaces.IChatSession')"
            inline="False"
            insert-before="++resource++tud.addons.chat/tud_chat.js" />

    <javascript id="++resource++tud.addons.chat/jquery.notification.js"
            cacheable="True"
